Create User 'TreeHole'@'%' Identified By 'treehole';

Create Role Server;

Use Server;

Drop Table Logs;
Create Table User(
    User_Id bigint primary key Auto_Increment,
    User_Name nvarchar(255) Not Null Unique,
    User_Password nchar(255) Not Null
);
Create Table Logs(
    User_Id bigint,
    Foreign key (User_Id) References User(User_Id),
    log_num bigint primary key Auto_Increment,
    log_time datetime Not Null,
    log_ipInfo varchar(255)
);
Create Table UserInfo(
    UsereInfo_Id bigint Primary Key Auto_Increment,
    User_id bigint,
    foreign key (User_Id) References User(User_Id),
    User_Email varchar(20),
    User_Phone char(11)
);
grant Update,insert On Logs To Server;
grant Update,insert On User To Server;
grant Update,insert On UserInfo To Server;
